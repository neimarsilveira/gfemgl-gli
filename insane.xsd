      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ShiftStandardIncidence" type="xs:string" />

  <xs:element name="Element">
    <xs:complexType>
      <xs:all>
        <xs:element ref="Incidence" />
        <xs:element ref="AnalysisModel" />
        <xs:element minOccurs="0" ref="IntegrationOrder" />
        <xs:element minOccurs="0" ref="ConstitutiveModel" />
        <xs:element minOccurs="0" ref="ElementInternalValues" />
        <xs:element minOccurs="0" ref="ElmDegenerations" />
        <xs:element minOccurs="0" ref="ElmDegenerationList" />
        <xs:element minOccurs="0" ref="ElementNodeValues" />
        <xs:element minOccurs="0" ref="ValuesInterpolationFrom" />
        <xs:element minOccurs="0" ref="GlobalElement" />
        <xs:element minOccurs="0" ref="BoundaryInformation" />
        <xs:element minOccurs="0" ref="ProblemDriver" />
        <xs:element minOccurs="0" ref="LayerList" />
        <xs:element minOccurs="0" ref="Angle" />
        <xs:element minOccurs="0" ref="VoronoiVertices" />
        <!-- BEM begin -->
        <xs:element minOccurs="0" ref="TractionPrescribed" />
        <!-- BEM end -->
        <xs:element minOccurs="0" ref="ElementCrackPropagation" />
      </xs:all>
      <xs:attribute name="class" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
      <xs:attribute name="info" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Incidence" type="xs:string" />

  <xs:element name="VoronoiVertices">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="VoronoiVertice" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="VoronoiVertice">
    <xs:complexType>
      <xs:all>
        <xs:element ref="VCoord" />
      </xs:all>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="VCoord" type="xs:string" />
  <xs:element name="AnalysisModel" type="xs:NCName" />
  <xs:element name="IntegrationOrder" type="xs:string" />
  <xs:element name="ConstitutiveModel" type="xs:NCName" />
  <xs:element name="ValuesInterpolationFrom" type="xs:string" />
  <xs:element name="GlobalElement" type="xs:string" />
  <xs:element name="BoundaryInformation" type="xs:string" />

  <xs:element name="ElementInternalValues">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="IntVarLabels" />
        <xs:element minOccurs="0" ref="DualIntVarLabels" />
        <xs:element minOccurs="0" ref="ConstitutiveVariablesLabels" />
        <xs:element minOccurs="0" ref="DamageVarLabels" />
        <xs:element minOccurs="0" ref="EquivalentStrainLabels" />
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="IntVarLabels" type="xs:string" />
  <xs:element name="DualIntVarLabels" type="xs:string" />
  <xs:element name="ConstitutiveVariablesLabels" type="xs:string" />
  <xs:element name="DamageVarLabels" type="xs:string" />
  <xs:element name="EquivalentStrainLabels" type="xs:string" />
  <xs:element name="ElmDegenerations" type="xs:string" />

  <xs:element name="ElmDegenerationList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="ElmDegeneration" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ElmDegeneration">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="IPCoord" />
        <xs:element ref="GeneralizedStrains" />
        <xs:element ref="GeneralizedStresses" />
        <xs:element ref="DiscontinuitySurface" />
        <xs:element ref="Damage" />
        <xs:element ref="EquivalentStrain" />
        <xs:element ref="TemperatureGradient" />
        <xs:element ref="ThermalFlux" />
        <xs:element ref="MPIntVarLabels" />
        <xs:element ref="MPDualIntVarLabels" />
        <xs:element maxOccurs="unbounded" ref="SectionPoint" />
      </xs:choice>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="IPCoord" type="xs:string" />
  <xs:element name="GeneralizedStrains" type="xs:string" />
  <xs:element name="GeneralizedStresses" type="xs:string" />
  <xs:element name="DiscontinuitySurface" type="xs:string" />
  <xs:element name="Damage" type="xs:string" />
  <xs:element name="EquivalentStrain" type="xs:string" />
  <xs:element name="TemperatureGradient" type="xs:string" />
  <xs:element name="MPIntVarLabels" type="xs:string" />
  <xs:element name="MPDualIntVarLabels" type="xs:string" />
  <xs:element name="SectionPoint">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="Strains" />
        <xs:element minOccurs="0" ref="Stresses" />
      </xs:all>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Strains" type="xs:string" />
  <xs:element name="Stresses" type="xs:string" />

  <xs:element name="ElementNodeValues">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ElmNode" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Liberations" />
        <xs:element minOccurs="0" maxOccurs="unbounded"
          ref="ElmPreDisplacements" />
        <xs:element minOccurs="0" maxOccurs="unbounded"
          ref="ElmPreTemperature" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <!-- Begin layer_LIST -->
  <xs:element name="LayerList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="Layer" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- End layer_LIST -->

  <!-- begin layer schema -->
  <xs:element name="Layer">
    <xs:complexType>
      <xs:all>
        <xs:element ref="Shape" />
        <xs:element ref="Incidence" />
        <xs:element minOccurs="0" ref="IntegrationOrder" />
        <xs:element ref="AnalysisModel" />
        <xs:element ref="ConstitutiveModel" />
        <xs:element ref="LayerDegenerations" />
        <xs:element minOccurs="0" ref="BondAnalysisModel" />
        <xs:element minOccurs="0" ref="BondConstitutiveModel" />
        <xs:element minOccurs="0" ref="BondDegenerations" />
        <xs:element minOccurs="0" ref="LayerInternalValues" />
        <xs:element minOccurs="0" ref="ElementLayerNodeValues" />
        <xs:element minOccurs="0" ref="ElmLayerDegenerationList" />
      </xs:all>
      <xs:attribute name="class" type="xs:string" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
      <xs:attribute name="info" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>
  <!-- End layer schema -->

  <xs:element name="Shape" type="xs:string" />
  <xs:element name="LayerDegenerations" type="xs:string" />
  <xs:element name="BondAnalysisModel" type="xs:NCName" />
  <xs:element name="BondConstitutiveModel" type="xs:NCName" />
  <xs:element name="BondDegenerations" type="xs:string" />

  <xs:element name="LayerInternalValues">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="IntVarLabels" />
        <xs:element minOccurs="0" ref="DualIntVarLabels" />
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="ElementLayerNodeValues">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ElmLayerNode" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ElmLayerNode">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="NodalStrains" />
        <xs:element minOccurs="0" ref="NodalStress" />
      </xs:all>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NodalStrains" type="xs:string" />
  <xs:element name="NodalStress" type="xs:string" />

  <xs:element name="ElmLayerDegenerationList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="layerDegeneration" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="layerDegeneration">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="IPCoord" />
        <xs:element ref="LayerStrains" />
        <xs:element ref="LayerStresses" />
      </xs:choice>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="LayerStrains" type="xs:string" />
  <xs:element name="LayerStresses" type="xs:string" />

  <!--Bem Model begin -->
  <xs:element name="TractionPrescribed">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ElmBemNode" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--Bem Model end -->

  <xs:element name="ElmNode">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="NodalGeneralizedStrains" />
        <xs:element minOccurs="0" ref="NodalGeneralizedStresses" />
        <xs:element minOccurs="0" ref="NodalConstitutiveVariables" />
        <xs:element minOccurs="0" ref="NodalDamage" />
        <xs:element minOccurs="0" ref="NodalEquivalentStrain" />
        <xs:element minOccurs="0" ref="NodalTemperatureGradient" />
        <xs:element minOccurs="0" ref="NodalThermalFlux" />
      </xs:all>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NodalGeneralizedStrains" type="xs:string" />
  <xs:element name="NodalGeneralizedStresses" type="xs:string" />
  <xs:element name="NodalDamage" type="xs:string" />
  <xs:element name="NodalConstitutiveVariables" type="xs:string" />
  <xs:element name="NodalEquivalentStrain" type="xs:string" />
  <xs:element name="NodalTemperatureGradient" type="xs:string" />
  <xs:element name="NodalThermalFlux" type="xs:string" />

  <xs:element name="Liberations">
