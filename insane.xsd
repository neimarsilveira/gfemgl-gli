  <!-- ************************************************************************************************************************** -->
  <xs:element name="NonLinearConstitutiveModel">
    <xs:complexType>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for LOADING_LIST -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="LoadingList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Loading" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Loading">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="NodeLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="InitialEquivalentNodalLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="PointLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="LineLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="AreaLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="VolumeLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="LineInitialInternalVariableLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="AreaInitialInternalVariableLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="VolumeInitialInternalVariableLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="LineTemperatureVariationLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="AreaTemperatureVariationLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="VolumeTemperatureVariationLoad" />
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="ThermalLoad" />
      </xs:sequence>
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NodeLoad">
    <xs:complexType mixed="true">
      <xs:attribute name="node" type="xs:double" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="InitialEquivalentNodalLoad">
    <xs:complexType mixed="true">
      <xs:attribute name="elm" type="xs:string" use="required" />
      <xs:attribute name="node" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="PointLoad">
    <xs:complexType>
      <xs:all>
        <xs:element ref="Point" />
        <xs:element ref="Load" />
      </xs:all>
      <xs:attribute name="elm" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Point" type="xs:string" />
  <xs:element name="Load" type="xs:string" />

  <xs:element name="LineLoad">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="PointLoad" />
      </xs:sequence>
      <xs:attribute name="elm" type="xs:string" use="required" />
      <xs:attribute name="intOrder" type="xs:integer" use="optional" />
    </xs:complexType>
  </xs:element>

  <xs:element name="AreaLoad">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0"
          ref="PointLoad" />
      </xs:sequence>
      <xs:attribute name="elm" type="xs:string" use="required" />
