  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for Insane -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="Insane">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="1" ref="Solution" />
        <xs:element minOccurs="1" ref="Model" />
        <xs:element minOccurs="0" ref="LoadingList" />
        <xs:element minOccurs="0" ref="RestraintsList" />
        <xs:element minOccurs="0" ref="ScalarFunctions" />
        <xs:element minOccurs="0" ref="LoadCombinations" />
        <xs:element minOccurs="0" ref="RestraintCombinations" />
      </xs:all>
    </xs:complexType>
  </xs:element>

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for Solution -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="Solution">
    <xs:complexType>
      <xs:all>
      	<xs:element minOccurs="0" ref="CycleControl"/>
        <xs:element minOccurs="0" ref="Increment" />
        <xs:element minOccurs="0" ref="LoadingPath" />
        <xs:element minOccurs="0" ref="ElementSectionsList" />
        <xs:element minOccurs="0" ref="CurrentPath" />
        <xs:element minOccurs="0" ref="ElementSections" />
        <xs:element minOccurs="0" ref="PositionSections" />
        <xs:element minOccurs="0" ref="NumMaxSteps" />
        <xs:element minOccurs="0" ref="PrintSteps" />
        <xs:element minOccurs="0" ref="Step" />
        <xs:element minOccurs="0" ref="ToleranceControl" />
        <xs:element minOccurs="0" ref="IterativeStrategyList" />
        <xs:element minOccurs="0" ref="ReturnMappingAlgorithm" />
        <xs:element minOccurs="0" ref="StepNumber" />
        <xs:element minOccurs="0" ref="FinalLoadFactor" />
        <xs:element minOccurs="0" ref="ModalProblem" />
        <xs:element minOccurs="0" ref="StepStartTime" />
        <xs:element minOccurs="0" ref="StepEndTime" />
        <xs:element minOccurs="0" ref="EndTime" />
        <xs:element minOccurs="0" ref="TimeStepLength" />
        <xs:element minOccurs="0" ref="HilberFactor" />
        <xs:element minOccurs="0" ref="NewmarkFactor" />
        <xs:element minOccurs="0" ref="NumericalDampingFactor" />
        <xs:element minOccurs="0" ref="ThetaFactor" />
        <xs:element minOccurs="0" ref="DampingType" />
        <xs:element minOccurs="0" ref="IterationMethod" />
        <xs:element minOccurs="0" ref="NumMaxIterations" />
        <xs:element minOccurs="0" ref="ConvergenceType" />
        <xs:element minOccurs="0" ref="EquilibriumType" />
        <xs:element minOccurs="0" ref="Tolerance" />
        <xs:element minOccurs="0" ref="IncrementalFactor" />
        <xs:element minOccurs="0" ref="IterationControl" />
        <xs:element minOccurs="0" ref="ToleranceLimit" />
        <xs:element minOccurs="0" ref="SolverType" />
        <xs:element minOccurs="0" ref="GlobalEnrichedNodes" />
        <xs:element minOccurs="0" ref="LocalEnricherList" />
        <xs:element minOccurs="0" ref="LocalPath" />
        <xs:element minOccurs="0" ref="LoadFactorMultiplier" />
        <xs:element minOccurs="0" ref="GlobalSolverType" />
        <xs:element minOccurs="0" ref="LocalSolverType" />
        <xs:element minOccurs="0" ref="GlobalLocalSolverType" />
        <xs:element minOccurs="0" ref="GlobalLocalIterations" />
        <xs:element minOccurs="0" ref="crackTipPoint" />
        <xs:element minOccurs="0" ref="numberOfElementLayers" />
        <xs:element minOccurs="0" ref="integralJpath" />
        <xs:element minOccurs="0" ref="elementsIntersectedByCrack" />
        <xs:element minOccurs="0" ref="crackDirectionAngle" />
        <xs:element minOccurs="0" ref="crackModes" />
        <xs:element minOccurs="0" ref="InteractIntegRadiusMultiplier" />
      </xs:all>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="CycleControl">
    <xs:complexType>
      <xs:all>
      	<xs:element minOccurs="0" ref="NodeCycleControl"/>
      	<xs:element minOccurs="0" ref="DirectionCycleControl"/>
      	<xs:element minOccurs="0" ref="NumCycles"/>
      	<xs:element minOccurs="0" ref="TolCycles"/>
		<xs:element minOccurs="0" ref="Script"/>
      </xs:all>
      <xs:attribute name="type" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>
  
  <xs:element name="NodeCycleControl" type="xs:string" />
   
  <xs:element name="DirectionCycleControl" type="xs:string" />
  
  <xs:element name="NumCycles" type="xs:string" />
  <xs:element name="TolCycles" type="xs:string" />
  
  <xs:element name="Script">
    <xs:complexType>
      <xs:sequence>
      	<xs:element minOccurs="0" maxOccurs="unbounded" ref="Cycle" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
    <xs:element name="Cycle" type="xs:string" />
  
  
  <xs:element name="Increment" type="xs:double" />
  <xs:element name="LoadingPath" type="xs:string" />

  <xs:element name="ElementSectionsList">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ElementSections" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="ElementSections">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="PositionSections" />
      </xs:all>
      <xs:attribute name="elmLabel" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="CurrentPath">
    <xs:complexType>
      <xs:attribute name="elmLabel" type="xs:string" use="required" />
      <xs:attribute name="loadingCoord" type="xs:double"
        use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="PositionSections" type="xs:string" />
  <xs:element name="NumMaxSteps" type="xs:integer" />
  <xs:element name="PrintSteps" type="xs:integer" />
  <xs:element name="Step">
    <xs:complexType>
      <xs:all>
        <xs:element ref="NumMaxIterations" />
        <xs:element ref="Tolerance" />
        <xs:element ref="ConvergenceType" />
        <xs:element minOccurs="0" ref="EquilibriumType" />
        <xs:element minOccurs="0" ref="LoadFactorMultiplier" />
      </xs:all>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NumMaxIterations" type="xs:integer" />
  <xs:element name="Tolerance" type="xs:double" />
  <xs:element name="ConvergenceType" type="xs:integer" />
  <xs:element name="EquilibriumType" type="xs:string" />
  <xs:element name="LoadFactorMultiplier" type="xs:double" />

  <xs:element name="ToleranceControl">
    <xs:complexType>
      <xs:all>
        <xs:element ref="IncrementalFactor" />
        <xs:element ref="IterationControl" />
        <xs:element ref="ToleranceLimit" />
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="IncrementalFactor" type="xs:double" />
  <xs:element name="IterationControl" type="xs:integer" />
  <xs:element name="ToleranceLimit" type="xs:double" />

  <xs:element name="IterativeStrategyList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="IterativeStrategy" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="IterativeStrategy">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="NodeControl" />
        <xs:element minOccurs="0" ref="DirectionControl" />
      </xs:all>
      <xs:attribute name="LoadFactor" type="xs:double" use="required" />
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NodeControl" type="xs:string" />
  <xs:element name="DirectionControl" type="xs:string" />
  <xs:element name="StepNumber" type="xs:integer" />
  <xs:element name="FinalLoadFactor" type="xs:double" />
  <xs:element name="ReturnMappingAlgorithm" type="xs:string" />

  <xs:element name="ModalProblem">
    <xs:complexType>
      <xs:all>
        <xs:element ref="Precision" />
        <xs:element ref="NumberOfModes" />
        <xs:element minOccurs="0" ref="NaturalFrequencies" />
      </xs:all>
      <xs:attribute name="type" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Precision" type="xs:double" />
  <xs:element name="NumberOfModes" type="xs:integer" />
  <xs:element name="NaturalFrequencies" type="xs:string" />
