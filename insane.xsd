  <!-- XML Schema for CRACK_GROWTH_DIRECTION_LAW -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="CrackGrowthDirectionLaw">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="DeclineWeightedFunc" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="DeclineWeightedFunc" type="xs:double" />

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for CRACK_GROWTH_INCREMENT_LAW -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="CrackGrowthIncrementLaw">
    <xs:complexType>
      <xs:sequence>
