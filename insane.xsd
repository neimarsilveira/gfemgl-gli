  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for ELEMENT_LIST -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="ElementList">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="ShiftStandardIncidence" />
