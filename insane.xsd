  <xs:element name="ElmPreDisplacements">
    <xs:complexType mixed="true">
      <xs:attribute name="node" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="ElmPreTemperature">
    <xs:complexType mixed="true">
      <xs:attribute name="node" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="ElementCrackPropagation">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="1" ref="CrackFirstPoint" />
        <xs:element minOccurs="1" ref="CrackSecondPoint" />
      </xs:all>
    </xs:complexType>
  </xs:element>

  <xs:element name="CrackFirstPoint" type="xs:string" />
  <xs:element name="CrackSecondPoint" type="xs:string" />

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for GEOMETRY_DESCRIPTION -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="GeometryDescription" type="xs:NCName" />

  <!-- XML Schema for GEOMETRY_ENTITY_LIST -->
  <xs:element name="GeometryEntityList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="GeometryEntity" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="GeometryEntity">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="2" ref="Vertex" />
        <xs:element maxOccurs="1" ref="PiecewiseLinear" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="Vertex">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Coords" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Coords" type="xs:string" />

  <xs:element name="PiecewiseLinear">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Vertices" />
        <xs:element ref="GeoDescription" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="Vertices" type="xs:string" />
  <xs:element name="GeoDescription" type="xs:NCName" />

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for ENRICHMENT_FUNCTION_LIST -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="EnrichmentFunctionList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="EnrichmentFunction" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="EnrichmentFunction">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="EnrichedDofLabels" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="EnrichedDofLabels" type="xs:string" />

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for ENRICHMENT_ITEM_LIST -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="EnrichmentItemList">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="EnrichmentItem" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="EnrichmentItem">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="2" ref="CrackTip" />
        <xs:element maxOccurs="1" ref="CrackInterior" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="CrackTip">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GEVertex" />
        <xs:element minOccurs="0" ref="CTEnrichFns" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="GEVertex" type="xs:string" />
  <xs:element name="CTEnrichFns" type="xs:string" />

  <xs:element name="CrackInterior">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GELinear" />
        <xs:element ref="Tips" />
        <xs:element minOccurs="0" ref="InteractedElm" />
        <xs:element minOccurs="0" ref="CIEnrichFns" />
        <xs:element minOccurs="0" ref="ConstModel" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="GELinear" type="xs:string" />
  <xs:element name="Tips" type="xs:string" />
  <xs:element name="InteractedElm" type="xs:string" />
  <xs:element name="CIEnrichFns" type="xs:string" />

  <xs:element name="ConstModel">
    <xs:complexType>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

