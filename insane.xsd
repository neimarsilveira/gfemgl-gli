        <!-- BEM end -->
      </xs:all>
      <xs:attribute name="class" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>


  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for AUTOMATIC_NODAL_DISTRIBUTION -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="AutomaticNodalDistribution">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" ref="NodalDistributionType" />
      </xs:sequence>
      <xs:attribute name="class" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="NodalDistributionType">
    <xs:complexType>
      <xs:choice minOccurs="1" maxOccurs="unbounded">
        <xs:element maxOccurs="1" minOccurs="0" ref="dx" />
        <xs:element maxOccurs="1" minOccurs="0" ref="dy" />
        <xs:element maxOccurs="1" minOccurs="1"
          ref="NumberOfNodesAtBorder" />
      </xs:choice>
      <xs:attribute name="class" type="xs:NCName" use="required" />
      <xs:attribute name="label" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>

  <xs:element name="dx" type="xs:double" />
  <xs:element name="dy" type="xs:double" />
  <xs:element name="NumberOfNodesAtBorder" type="xs:string" />

  <!-- ************************************************************************************************************************** -->
  <!-- XML Schema for NONLOCAL_PARAMETERS -->
  <!-- ************************************************************************************************************************** -->
  <xs:element name="NonLocalParameters">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="DistributionFunction" />
