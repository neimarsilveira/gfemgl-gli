  <xs:element name="NewmarkFactor" type="xs:double" />
  <xs:element name="NumericalDampingFactor" type="xs:double" />
  <xs:element name="ThetaFactor" type="xs:double" />
  <xs:element name="IterationMethod" type="xs:string" />

  <xs:element name="DampingType">
    <xs:complexType>
      <xs:all>
        <xs:element minOccurs="0" ref="ModalDampingVector" />
        <xs:element minOccurs="0" ref="RayleighCoefficients" />
      </xs:all>
      <xs:attribute name="type" type="xs:NCName" use="required" />
    </xs:complexType>
  </xs:element>

