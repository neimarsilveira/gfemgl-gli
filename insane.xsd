  <xs:element name="PreVelocities" type="xs:string" />
  <xs:element name="Mass" type="xs:string" />
  <xs:element name="Damping" type="xs:string" />
  <xs:element name="Stiffness" type="xs:string" />
  <xs:element name="Displacements" type="xs:string" />
  <xs:element name="Velocities" type="xs:string" />
  <xs:element name="Accelerations" type="xs:string" />
  <xs:element name="Reactions" type="xs:string" />
  <xs:element name="PreTemperature" type="xs:string" />
  <xs:element name="Temperature" type="xs:string" />
  <xs:element name="ThermalStiffness" type="xs:string" />
  <xs:element name="ThermalGradient" type="xs:string" />
  <xs:element name="TemperatureFirstTimeDerivate" type="xs:string" />
  <xs:element name="TemperatureSecondTimeDerivate" type="xs:string" />
  <xs:element name="ThermalFlux" type="xs:string" />
  <xs:element name="EnrichmentType" type="xs:string" />
  <xs:element name="ScalingFactor" type="xs:string" />
  <xs:element name="Strain" type="xs:string" />
  <xs:element name="Stress" type="xs:string" />
